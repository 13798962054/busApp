/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.personalIcon{
  width: 200rpx;
  height: 200rpx;
  overflow: hidden;
  margin: auto;
}

.personalIcon image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}

.personalName{
  text-align: center;
  color: #ccc;
}

.line{
  line-height: 100rpx;
  margin: 15rpx 40rpx;
}
